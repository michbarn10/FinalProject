A;;AAAA;AAEA,gBAAA,QAFA;AAGA,gBAAA,MAAA,CAAA,OAAA,GAAA,QAAA,CAAA,IAAA;;AAHA,qBAIA,MAAA,CAAA,OAJA;AAAA;AAAA;AAAA;;AAAA;AAAA,uBAKA,KAAA,CAAA,GAAA,CAAA,aAAA,CALA;;AAAA;AAKA,gBAAA,aALA;;AAMA,gBAAA,MAAA,CAAA,OAAA,CAAA,OAAA,CAAA,UAAA,MAAA,EAAA;AACA,sBAAA,WAAA,GAAA,MAAA,CAAA,kBAAA,CAAA,MAAA,CAAA,OAAA,CAAA;;AACA,sBAAA,CAAA,WAAA,EAAA;AACA,oBAAA,MAAA,CAAA,MAAA,CAAA,IAAA,CACA,aAAA,CAAA,IAAA,CAAA,IAAA,CAAA,UAAA,KAAA;AAAA,6BAAA,KAAA,CAAA,EAAA,IAAA,MAAA,CAAA,OAAA;AAAA,qBAAA,CADA;AAGA;AACA,iBAPA;;AANA;AAAA,kDAeA,IAfA;;AAAA;AAAA;AAAA;AAiBA,gBAAA,OAAA,CAAA,GAAA;;AAjBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAmBA,KApBA;AAqBA,IAAA,MArBA,oBAqBA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,KAAA,UAAA,CAAA,YAAA,CAFA;;AAAA;AAGA,gBAAA,MAAA,CAAA,KAAA,CAAA,KAAA,CAAA,IAAA,GAAA,IAAA;AAHA;AAAA;;AAAA;AAAA;AAAA;AAKA,gBAAA,OAAA,CAAA,GAAA;AACA,gBAAA,MAAA,CAAA,KAAA,CAAA,KAAA,CAAA,IAAA,GAAA,IAAA;;AANA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAQA,KA7BA;AA8BA,IAAA,kBA9BA,8BA8BA,OA9BA,EA8BA;AACA,aAAA,KAAA,MAAA,CAAA,IAAA,CAAA,UAAA,KAAA;AAAA,eAAA,KAAA,CAAA,EAAA,IAAA,OAAA;AAAA,OAAA,CAAA;AACA;AAhCA;AAzBA,CAAA","sourcesContent":["<template>\n  <div>\n    <div id=\"menu\">\n      <div id=\"brand\">\n        <router-link to=\"/\">\n          <img src=\"../assets/globe.jpeg\" />\n        </router-link>\n      </div>\n      <div id=\"side\">\n        <button v-if=\"user\" @click=\"logout\">Sign Out</button>\n        <router-link to=\"/\">\n          <div class=\"menu-item home\">\n            <img src=\"../assets/llamas.jpeg\" />\n            <p>Home</p>\n          </div>\n        </router-link>\n        <router-link to=\"/reviews\">\n          <div class=\"menu-item\">\n            <img src=\"../assets/tv.png\" />\n            <p>Reviews</p>\n          </div>\n        </router-link>\n      </div>\n    </div>\n    <div v-if=\"this.reviews.length == 0\">\n      <h1>There are no reviews to view.</h1>\n    </div>\n    <div v-else><Review-List /></div>\n    <!-- <div v-else><Review-List :reviews=\"reviews\" :movies=\"movies\" /> -->\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport ReviewList from \"../components/ReviewList.vue\";\nexport default {\n  name: \"Reviews\",\n  components: {\n    ReviewList,\n  },\n  data() {\n    return {\n      reviews: [],\n      movies: [],\n    };\n  },\n  async created() {\n    this.getReviews();\n    try {\n      let response = await axios.get(\"/api/users\");\n      this.$root.$data.user = response.data.user;\n    } catch (error) {\n      this.$root.$data.user = null;\n    }\n  },\n  computed: {\n    user() {\n      return this.$root.$data.user;\n    },\n  },\n  methods: {\n    async getReviews() {\n      try {\n        let response = await axios.get(\"/api/reviews\");\n        this.reviews = response.data;\n        if (this.reviews) {\n          let movieResponse = await axios.get(\"/api/movies\");\n          this.reviews.forEach((review) => {\n            const movieExists = this.getMovieFromReview(review.movieId);\n            if (!movieExists) {\n              this.movies.push(\n                movieResponse.data.find((movie) => movie.id == review.movieId)\n              );\n            }\n          });\n        }\n        return true;\n      } catch (error) {\n        console.log(error);\n      }\n    },\n    async logout() {\n      try {\n        await axios.delete(\"/api/users\");\n        this.$root.$data.user = null;\n      } catch (error) {\n        console.log(error);\n        this.$root.$data.user = null;\n      }\n    },\n    getMovieFromReview(movieId) {\n      return this.movies.find((movie) => movie.id == movieId);\n    },\n  },\n};\n</script>\n\n<style scoped>\nh1 {\n  font-size: 16px;\n  background: #8b0000;\n}\n</style>\n"],"sourceRoot":"src/views"}]}