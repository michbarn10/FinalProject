KAAA,CAAA,kBAAA,CAAA,MAAA,CAAA,OAAA,CAAA;;AACA,sBAAA,CAAA,WAAA,EAAA;AACA,oBAAA,KAAA,CAAA,MAAA,CAAA,IAAA,CACA,aAAA,CAAA,IAAA,CAAA,IAAA,CAAA,UAAA,KAAA;AAAA,6BAAA,KAAA,CAAA,EAAA,IAAA,MAAA,CAAA,OAAA;AAAA,qBAAA,CADA;AAGA;;AAEA,kBAAA,KAAA,CAAA,QAAA,CAAA,IAAA,CAAA,KAAA;AACA,iBATA;;AANA;AAAA,iDAiBA,IAjBA;;AAAA;AAAA;AAAA;AAmBA,gBAAA,OAAA,CAAA,GAAA;;AAnBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAqBA,KA1BA;AA2BA,IAAA,YA3BA,wBA2BA,KA3BA,EA2BA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,KAAA,UAAA,CAAA,iBAAA,MAAA,CAAA,OAAA,CAAA,KAAA,EAAA,GAAA,CAFA;;AAAA;AAGA,gBAAA,MAAA,CAAA,OAAA,CAAA,MAAA,CAAA,KAAA,EAAA,CAAA;;AAHA,kDAIA,IAJA;;AAAA;AAAA;AAAA;AAMA,gBAAA,OAAA,CAAA,GAAA;;AANA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAQA,KAnCA;AAoCA,IAAA,kBApCA,8BAoCA,OApCA,EAoCA;AACA,aAAA,KAAA,MAAA,CAAA,IAAA,CAAA,UAAA,KAAA;AAAA,eAAA,KAAA,CAAA,EAAA,IAAA,OAAA;AAAA,OAAA,CAAA;AACA,KAtCA;AAuCA,IAAA,eAvCA,2BAuCA,MAvCA,EAuCA;AACA,UAAA,YAAA,GAAA,KAAA,kBAAA,CAAA,MAAA,CAAA,OAAA,CAAA;;AACA,UAAA,YAAA,EAAA;AACA,eAAA,YAAA,CAAA,KAAA;AACA,OAFA,MAEA;AACA,eAAA,EAAA;AACA;AACA,KA9CA;AA+CA,IAAA,UA/CA,sBA+CA,KA/CA,EA+CA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,KAAA,CAAA,GAAA,CAAA,iBAAA,MAAA,CAAA,OAAA,CAAA,KAAA,EAAA,GAAA,EAAA;AACA,kBAAA,UAAA,EAAA,MAAA,CAAA,UAAA,CAAA,KAAA;AADA,iBAAA,CAFA;;AAAA;AAAA;AAAA,uBAMA,KAAA,CAAA,GAAA,CAAA,cAAA,CANA;;AAAA;AAMA,gBAAA,QANA;AAOA,gBAAA,MAAA,CAAA,OAAA,GAAA,QAAA,CAAA,IAAA;;AACA,gBAAA,MAAA,CAAA,UAAA,CAAA,KAAA;;AARA;AAAA;;AAAA;AAAA;AAAA;AAUA,gBAAA,OAAA,CAAA,GAAA;;AAVA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAYA;AA3DA;AAbA,CAAA","sourcesContent":["<template>\n  <div class=\"wrapper\">\n    <div class=\"reviews\">\n      <div class=\"review\" v-for=\"(review, index) in reviews\" :key=\"review.id\">\n        <div class=\"image\">\n          <img :src=\"require(`@/assets/Movies/${getMoviePicture(review)}`)\" />\n        </div>\n        <div class=\"info\">\n          <h1>{{ review.reviewName }}</h1>\n          <h2>{{ review.userReview }}</h2>\n        </div>\n        <div class=\"name\">\n          <h3>{{ review.movieName }}</h3>\n        </div>\n        <div\n          v-if=\"v-if\"\n          =\"\n            user && user.firstName + ' ' + user.lastName == review.reviewName\n          \"\n          class=\"deleteReview\"\n        >\n          <button @click=\"deleteReview(index)\">Delete this review</button>\n        </div>\n        <div\n          v-if=\"\n            user && user.firstName + ' ' + user.lastName == review.reviewName\n          \"\n          class=\"deleteReview\"\n        >\n          <button @click=\"toggleEdit(index)\">Edit this review</button>\n        </div>\n        <div class=\"review\" v-if=\"showEdit[index]\">\n          <input\n            type=\"text\"\n            v-model=\"userReview[index]\"\n            placeholder=\"Type the new text here...\"\n          />\n          <button @click=\"editReview(index)\">Update Your Review</button>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nexport default {\n  name: \"ReviewList\",\n  data() {\n    return {\n      reviews: [],\n      movies: [],\n      showEdit: [],\n      userReview: [],\n    };\n  },\n  created() {\n    this.getReviews();\n  },\n  methods: {\n    toggleEdit(index) {\n      const oldVal = this.showEdit[index];\n      this.showEdit.splice(index, 1, !oldVal);\n    },\n    async getReviews() {\n      try {\n        let response = await axios.get(\"/api/reviews\");\n        this.reviews = response.data;\n        if (this.reviews) {\n          let movieResponse = await axios.get(\"/api/movies\");\n          this.reviews.forEach((review) => {\n            const movieExists = this.getMovieFromReview(review.movieId);\n            if (!movieExists) {\n              this.movies.push(\n                movieResponse.data.find((movie) => movie.id == review.movieId)\n              );\n            }\n\n            this.showEdit.push(false);\n          });\n        }\n        return true;\n      } catch (error) {\n        console.log(error);\n      }\n    },\n    async deleteReview(index) {\n      try {\n        await axios.delete(\"/api/Review/\" + this.reviews[index]._id);\n        this.reviews.splice(index, 1);\n        return true;\n      } catch (error) {\n        console.log(error);\n      }\n    },\n    getMovieFromReview(movieId) {\n      return this.movies.find((movie) => movie.id == movieId);\n    },\n    getMoviePicture(review) {\n      let moviePicture = this.getMovieFromReview(review.movieId);\n      if (moviePicture) {\n        return moviePicture.image;\n      } else {\n        return \"\";\n      }\n    },\n    async editReview(index) {\n      try {\n        await axios.put(\"/api/Review/\" + this.reviews[index]._id, {\n          userReview: this.userReview[index],\n        });\n\n        let response = await axios.get(\"/api/reviews\");\n        this.reviews = response.data;\n        this.toggleEdit(index);\n      } catch (error) {\n        console.log(error);\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\n.wrapper {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n}\n\n.reviews {\n  margin-top: 20px;\n  display: flex;\n  flex-wrap: wrap;\n  justify-content: space-around;\n}\n\n.review {\n  margin: 10px;\n  margin-top: 50px;\n  width: 200px;\n}\n\n.review img {\n  border: 2px solid #333;\n  height: 250px;\n  width: 200px;\n  object-fit: cover;\n}\n\n.review .image {\n  display: flex;\n  justify-content: center;\n  margin-bottom: 7px;\n}\n\n.info {\n  background: #8b0000;\n  border-radius: 5px;\n  color: rgb(255, 255, 255);\n  padding: 10px 30px;\n  height: 110px;\n}\n\n.info h1 {\n  font-size: 16px;\n  background: #8b0000;\n}\n\n.info h2 {\n  font-size: 14px;\n  background: #8b0000;\n}\n\n.info p {\n  margin: 0px;\n  font-size: 10px;\n  background: #8b0000;\n}\n\nbutton {\n  height: 50px;\n  background: #8b0000;\n  border-radius: 5px;\n  color: white;\n  border: none;\n  font-family: \"Lucida Console\", \"Courier New\", monospace;\n}\n.deleteReview {\n  margin: 5px;\n}\n\n.auto {\n  margin-left: auto;\n}\n.review .reviewName input {\n  width: 100%;\n  height: 30px;\n  margin-bottom: 2px;\n  border-radius: 5px;\n}\n.review {\n  justify-content: center;\n}\n.review input {\n  width: 100%;\n  height: 70px;\n  border-radius: 5px;\n  margin-top: 5px;\n}\n.review button {\n  height: 40px;\n  background: #8b0000;\n  color: rgb(255, 255, 255);\n  border-radius: 5px;\n  font-family: \"Lucida Console\", \"Courier New\", monospace;\n  border: none;\n  align-items: center;\n  width: 90%;\n  margin: 5px;\n}\n</style>\n"],"sourceRoot":"src/components"}]}